#include<iostream>
#include<math.h>
using namespace std ;



bool kunal(int yash[] , int n,int key){

  for (int i = 0; i <n; i++)
  {
    if (yash[i] == key)
    {
      return 1;
    }
    
  }
  return 0;
  
}


int main()
{
 int yash[10]= {1,45,75,334,6554,35,66,34,56,23};

cout<<"enter which element you want to find"<<endl;
int key;
cin>>key;

 bool pawan = kunal(yash,10,key);

if (pawan)
{
  cout<<"element found";
}
else{
  cout<<"element not found";
}
 }
 

